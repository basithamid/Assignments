package com.hibernate.mapping;

import javax.persistence.*;

@Entity
@Table(name="EMP")
public class Emp {
	@Id
	@GeneratedValue
	@Column(name="EMPNO")
	private int id;
	
	@Column(name="ENAME")
	private String name;
	
	
	@ManyToOne
	@JoinColumn(name="MGR", referencedColumnName="EMPNO")
	private Emp mgrId;
	
	
	
	
}
