package exception;
class ABC{
	public void print(){
		System.out.println("Hi");
	}
}
public class ExceptionForms {
	public static void main(String args[]){
		int a = 5, b = 0;
		ABC abc=null;
		try{
//			int c = a / b;
//			System.out.println(c);
			abc.print();
		}
		catch(Exception e){
			e.printStackTrace();
		}
		catch(ArithmeticException e){
			e.printStackTrace();
		}
		
		System.out.println("Bye");
	}
}
