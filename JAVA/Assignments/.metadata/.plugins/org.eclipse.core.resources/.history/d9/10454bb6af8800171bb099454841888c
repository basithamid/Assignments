package com.spring.FirstSpringDemo1;

import java.util.HashMap;

import org.springframework.context.support.AbstractApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;
/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args )
    {
        AbstractApplicationContext iocContainer = new ClassPathXmlApplicationContext("Beans_simple.xml");
        
        HashMap <String,OrderBean> orders = new HashMap<String, OrderBean>();
        
        OrderBean order = (OrderBean)iocContainer.getBean("order_1");
        System.out.println("Order: " + order.getOrderID());
    }
}
