package com.spring.advertisement.entity;

public class User {
	private int id;
	private String name;
	private String city;
	private String uname;
	private String pwd;
	public User(){}
	public User(int id, String name, String city, String uname, String pwd) {
		super();
		this.id = id;
		this.name = name;
		this.city = city;
		this.uname = uname;
		this.pwd = pwd;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getCity() {
		return city;
	}
	public void setCity(String city) {
		this.city = city;
	}
	public String getUname() {
		return uname;
	}
	public void setUname(String uname) {
		this.uname = uname;
	}
	public String getPwd() {
		return pwd;
	}
	public void setPwd(String pwd) {
		this.pwd = pwd;
	}
	
	
}
